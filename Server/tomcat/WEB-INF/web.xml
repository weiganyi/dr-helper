<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
    version="2.4">

	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.drhelper.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.drhelper.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CheckTableServlet</servlet-name>
        <servlet-class>com.drhelper.servlet.CheckTableServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckTableServlet</servlet-name>
        <url-pattern>/checkTable.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateTableServlet</servlet-name>
        <servlet-class>com.drhelper.servlet.CreateTableServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateTableServlet</servlet-name>
        <url-pattern>/createTable.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoadOrderServlet</servlet-name>
        <servlet-class>com.drhelper.servlet.LoadOrderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadOrderServlet</servlet-name>
        <url-pattern>/loadOrder.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateMenuTypeServlet</servlet-name>
        <servlet-class>com.drhelper.servlet.UpdateMenuTypeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateMenuTypeServlet</servlet-name>
        <url-pattern>/updateMenuType.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateMenuServlet</servlet-name>
        <servlet-class>com.drhelper.servlet.UpdateMenuServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateMenuServlet</servlet-name>
        <url-pattern>/updateMenu.do</url-pattern>
    </servlet-mapping>

</web-app>
